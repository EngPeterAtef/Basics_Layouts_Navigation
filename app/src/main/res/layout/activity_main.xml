        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to RelativeLayout" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Open Calculator" />

</LinearLayout>
